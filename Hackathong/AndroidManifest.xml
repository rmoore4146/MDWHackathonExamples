<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="dix.walton.moore"
	android:versionCode="10" android:versionName="2.3.3">
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:name=".activity.GMapsActivity" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
        <activity android:name=".activity.AndroidWebService" android:label="@string/app_name"/>

        <!--
        ===================================
        UNCOMMENT TO ENABLE GOOGLE MAPS
        ===================================
        -->

        <uses-library android:name="com.google.android.maps" android:required="true"/>
	</application>

